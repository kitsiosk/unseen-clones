    char *buffer;
    int index = 0;
    p->Name = malloc (80);
    buffer = strtok (textLine, "|");
    while (buffer != NULL && strcmp (textLine, "\n")) {if (index == 0) {p -> Name = buffer;
    buffer = strtok (NULL, "|");
