char *readFile (char *fileName) {
    FILE *file;
    int i = 0;
    char *code = malloc (1000 * sizeof (char));
    file = fopen (fileName, "r");
    do {
        code[i++] = (char) fgetc (file);
    }
    while (code[i - 1] != EOF);
    code [i] = '\0' return code;
}

char *readFile (char *fileName) {
    FILE *file = fopen (fileName, "r");
    char *code;
    size_t n = 0;
    int c;
    if (file == NULL)
        return NULL;
    code = malloc (1000);
    while ((c = fgetc (file)) != EOF) {
        code[n++] = (char) c;
    }
    code[n] = '\0';
    return code;
}

